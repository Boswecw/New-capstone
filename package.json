{
  "name": "furbabies-petstore",
  "version": "1.0.0",
  "description": "FurBabies Pet Store MERN Application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "cd server && nodemon server.js",
    "client": "cd client && yarn start",
    "build": "cd client && yarn build",
    "install-server": "cd server && yarn install",
    "install-client": "cd client && yarn install",
    "install-all": "yarn install-server && yarn install-client",
    "render-postbuild": "yarn install-all && yarn build",
    "clean": "rm -rf node_modules server/node_modules client/node_modules yarn.lock",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "pet-adoption",
    "mern-stack",
    "react",
    "nodejs",
    "mongodb",
    "image-optimization"
  ],
  "author": "Charlie Boswell",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^6.10.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Boswecw/furbabies-petstore.git"
  },
  "bugs": {
    "url": "https://github.com/Boswecw/furbabies-petstore/issues"
  },
  "homepage": "https://github.com/Boswecw/furbabies-petstore#readme"
}
